pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                sh 'python --version'
            }
        }
      
       stage('build stage 2') {
            steps {
                sh 'echo "Stage 2"'
            }
        }
      
      
      
       stage('build stage 3') {
            steps {
                sh 'echo "Stage 3"'
            }
        }
      
      
    }
}
